{
    "extends": "../../tsconfig.json",
    "include": ["./src/**/*"],
    "compilerOptions": {
        "lib": ["DOM"],
        "baseUrl": "./",
        "jsx": "preserve",
        "jsxImportSource": "vue",
        "paths": {
            "src/*": ["src/*"]
